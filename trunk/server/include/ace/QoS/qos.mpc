project(QoS) : acelib, ace_output, install {
  requires    += qos
  sharedname   = ACE_QoS
  dynamicflags = ACE_QoS_BUILD_DLL
  macros      += ACE_HAS_QOS

  specific(borland, bmake, nmake, em3, vc6, vc7, vc71, vc8, vc9) {
    macros += ACE_HAS_WINSOCK2_GQOS
  }

  pkgconfig_files {
    ACE_QoS.pc.in
  }
}
